{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./dist",
    "allowJs": false,
    "declaration": true,
    "noUnusedLocals": true,
    "strictNullChecks": false,
    "sourceMap": true,
    "resolveJsonModule": true,
    "lib": ["esnext.asynciterable", "es6", "es7", "dom"],
    "pretty": true
  },
  "include": ["src/**/*"]
}
