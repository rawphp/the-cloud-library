{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "max-line-length": {
      "options": {
        "limit": 150
      }
    },
    "quotemark": [
      true,
      "single"
    ],
    "completed-docs": [
      true
    ],
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "space-before-function-paren": false,
    "no-console": false
  },
  "rulesDirectory": []
}
